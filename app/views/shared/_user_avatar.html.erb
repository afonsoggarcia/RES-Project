
<% if current_user.photo.attached? %>
  <%= cl_image_tag current_user.photo.key,:width=>50, :height=>50, :gravity=>"face", :crop=>"fill", class: "avatar dropdown-toggle", id: "navbarDropdown", data: { bs_toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %>
<% else %>
  <%= image_tag "planetinha.png", class: "avatar dropdown-toggle", id: "navbarDropdown", data: { bs_toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %>
<% end %>
